{
  "compilerOptions": {
    "declaration": true,
    "target": "ES2022",
    "module": "NodeNext",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "outDir": "./dist",
    "rootDir": "./src",
    "plugins": [
      { "name": "@effect/language-service" }
    ]
  },
  "include": ["src"]
}
